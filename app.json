{
  "expo": {
    "name": "NamazGostergem",
    "slug": "NamazGostergem",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMotionUsageDescription": "NamazGostergem uses motion sensors during prayer sessions to detect prayer movement and guide the user with haptic feedback."
      }
    },
    "android": {
      "package": "com.example.namazgostergem",
      "permissions": [
        "VIBRATE",
        "ACCESS_NOTIFICATION_POLICY",
        "WRITE_SETTINGS"
      ]
    }
  }
}
